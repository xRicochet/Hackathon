<div style="padding-left: 0px; padding-right: 0px; width: auto; margin-left:0px;margin-right:0px;">
    
    <div id="myCarousel" class="carousel slide">

        <!-- Carousel indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Carousel items -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="https://static.pexels.com/photos/2324/skyline-buildings-new-york-skyscrapers.jpg" alt="First slide">
            </div>

            <div class="item">
                <img src="https://static.pexels.com/photos/24367/pexels-photo-24367.jpg" alt="Second slide">
            </div>

            <div class="item">
                <img src="https://static.pexels.com/photos/2773/city-skyline-skyscrapers-top.jpg" alt="Third slide">
            </div>
        </div>

        <!-- Carousel nav -->
        <a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
        <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>

    </div> 
</div>
    <script>
        $(".owl-carousel").owlCarousel({
            loop: true,
            margin: 10,
            nav: true,
            autoHeight: true,
            autoWidth: true,
        })
    </script>
</div>

